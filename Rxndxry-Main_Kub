                Parry()
                Parry()
                Parry()
                Parry()
                Parry()
                Parry()
            end) 
        end 
	end
end)
Main:Toggle("Spectator Ball",_G.SpectBall,function(t)
    _G.SpectBall = t
end)
game:GetService("Workspace").Balls.ChildAdded:Connect(function(child) 
    local trackTask = task.spawn(function()
    local ball = child
    while task.wait() do
        if _G.SpectBall then
            local camera = game.workspace.CurrentCamera 
            camera.CameraSubject = ball
        else 
            local camera = game.workspace.CurrentCamera
            camera.CameraSubject = game.Players.LocalPlayer.Character.HumanoidRootPart
        end
    end
end)
    child.Destroying:Connect(function()
        task.cancel(trackTask)
    end)
end)


Main:Toggle("Teleport To Ball",_G.Tpball,function(t)
    _G.Tpball = t
end)
task.spawn(function()
    while task.wait() do
        if _G.Tpball then
            X = math.random(-15 , 15)
            Y = math.random(-15 , 15)
            Z = math.random(-15 , 15)
            if X < 10 or X > -10  or Y < 10 or Y > -10  or Z < 10 or Z > -10  then
                X = 15
                Y = 15
                Z = 15
            end
        end
    end
end)


spawn(function()
    while task.wait() do
        if _G.Tpball then
            pcall(function()
                for _,ball in next, workspace.Balls:GetChildren() do
                    if ball then
                        if _G.Tpball == true then    
                            game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = ball.CFrame * CFrame.new(X, Y, (ball.Velocity).Magnitude * Z)
                        else
                            game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = ball.CFrame * CFrame.new(15, 0, (ball.Velocity).Magnitude * 15)
                        end
                    end
                end
            end)
        end 
    end
end)

