local Time = Profile:Label("")

function UpdateTime()
    local gameTime = math.floor(workspace.DistributedGameTime + 0.5)
    local hour = math.floor(gameTime / (60^2)) % 24
    local minute = math.floor(gameTime / (60^1)) % 60
    local second = math.floor(gameTime / (60^0)) % 60
    Time:Change("\nHours : ".. hour .. "Minutes : " .. minute .. "Seconds : "..second)
end
task.spawn(function()
    while task.wait() do
        UpdateTime()
    end
end)
local Fps = Profile:Label("")

function UpdateFps()
    local fps = workspace:GetRealPhysicsFPS()
    Fps:Change("Fps: "..fps)
end
task.spawn(function()
    while task.wait() do
        UpdateFps()
    end
end)

local Ping = Profile:Label("")

function UpdatePing()
    local ping = game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValueString()
    Ping:Change("Ping: "..ping)
end
task.spawn(function()
    while task.wait() do
        UpdatePing()
    end
end)

